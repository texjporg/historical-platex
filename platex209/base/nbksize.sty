\newdimen\gyo@ht
\newdimen\gyo@dp

\def\gyo#1{\par%
  \bgroup
  \global\gyo@ht\Cht\global\gyo@dp\Cdp\relax
  \baselineskip=#1
  \advance\baselineskip\gyo@dp
  \dimen0\baselineskip
  \advance\dimen0 by -\normalbaselineskip
  \advance\dimen0\gyo@ht
  \dimen1\normalbaselineskip
  \advance\dimen1 by -\gyo@ht
  \vbox to \dimen0 \bgroup%
    \baselineskip=\normalbaselineskip
    \boxmaxdepth=0pt\lineskiplimit=0pt\lineskip=0pt
    \prevdepth=\dimen1
    \futurelet\next\@gyo}

\def\@gyo{%
  \ifcat\bgroup\noexpand\next \let\next\@@gyo\else\let\next\@@@gyo\fi\next}
\def\@@gyo{\bgroup\aftergroup\@gyoend\let\next}
\def\@@@gyo#1{#1\@gyoend}
\def\@gyoend{\par\kern0pt\egroup\egroup\prevdepth=\gyo@dp}

\def\chapter{%
  \cleardoublepage \thispagestyle{nohead} \global\@topnum\z@
  \@afterindenttrue \secdef\@chapter\@schapter} 

\def\@chapter[#1]#2{%
  \ifnum \c@secnumdepth >\m@ne
    \refstepcounter{chapter}
    \typeout{\@chapapp\space\t@thechapter\space\@chappos}
    \addcontentsline{toc}{chapter}{%
      \@chapapp\thechapter\@chappos}{#1}
  \else
    \addcontentsline{toc}{chapter}{}{#1}
  \fi
  \chaptermark{#1}
  \addtocontents{lof}{\protect\addvspace{10pt}}
  \addtocontents{lot}{\protect\addvspace{10pt}}
  \@makechapterhead{#2}
  \if@twocolumn\else
    \@afterheading
  \fi} 

\def\@schapter#1{%
  \@makeschapterhead{#1} 
  \if@twocolumn\else
    \@afterheading
  \fi}

\def\@makechapterhead#1{%
  \gyo{8\normalbaselineskip}{%
    \parindent\z@
    \vss
    \if@av
      \leftskip3zw
      \setbox0=\hbox{\Huge\gt\bf\@chapapp\thechapter\@chappos\hskip1zw}
      \advance\leftskip\wd0\hskip-\wd0\relax
      {\Huge\gt\bf\global\dimen0=14zw}%
      \rightskip\linewidth\advance\rightskip-\dimen0\advance\rightskip-\leftskip
      \box0\Huge\gt\bf #1
    \else
      \leftskip1zw
      \setbox0=\hbox{\huge\gt\bf\@chapapp\thechapter\@chappos\hskip1zw}
      \advance\leftskip\wd0\hskip-\wd0\relax
      {\huge\gt\bf\global\dimen0=8zw}%
      \rightskip\linewidth\advance\rightskip-\dimen0\advance\rightskip-\leftskip
      \box0\huge\gt\bf #1
    \fi
    \vss}}

\def\@makeschapterhead#1{%
  \gyo{8\normalbaselineskip}{%
    \parindent\z@
    \vss
    \if@av
      \hskip3zw
      \Huge\gt\bf #1
    \else
      \huge\gt\bf #1
    \fi
    \vss}}

\def\section{%
  \@afterindenttrue
  \secdef\@section\@ssection}

\def\@section[#1]#2{%
  \ifnum \c@secnumdepth >\m@ne
    \refstepcounter{section}
    \addcontentsline{toc}{section}{}{#1}
  \else
    \addcontentsline{toc}{section}{}{#1}
  \fi
  \sectionmark{#1}
  \addtocontents{lof}{\protect\addvspace{10pt}}
  \addtocontents{lot}{\protect\addvspace{10pt}}
  \@makesectionhead{#2}
  \if@twocolumn\else
    \@afterheading
  \fi} 

\def\@ssection#1{%
  \@makessectionhead{#1}
  \if@twocolumn\else
    \@afterheading
  \fi}

\def\@makesectionhead#1{%
  \vskip2\normalbaselineskip
  \gyo{2\normalbaselineskip}{%
    \parindent\z@
    \leftskip1zw
    \Large\gt\bf #1
    \vss}\nobreak}

\let\@makessectionhead=\@makesectionhead

\def\subsection{%
  \@afterindenttrue
  \secdef\@subsection\@subssection}

\def\@subsection[#1]#2{%
  \ifnum \c@secnumdepth >\m@ne
    \refstepcounter{subsection}
    \addcontentsline{toc}{subsection}{}{#1}
  \else
    \addcontentsline{toc}{subsection}{}{#1}
  \fi
  \addtocontents{lof}{\protect\addvspace{10pt}}
  \addtocontents{lot}{\protect\addvspace{10pt}}
  \@makesubsectionhead{#2}
  \if@twocolumn\else
    \@afterheading
  \fi} 

\def\@subssection#1{%
  \@makesubssectionhead{#1}
  \if@twocolumn\else
    \@afterheading
  \fi}

\def\@makesubsectionhead#1{%
  \gyo{2\normalbaselineskip}{%
    \parindent\z@
    \leftskip1zw
    \vss
    \large\gt\bf #1
    \vss}\nobreak}

\let\@makesubssectionhead=\@makesubsectionhead

\def\subsubsection{%
  \@afterindenttrue
  \secdef\@subsubsection\@subsubssection}

\def\@subsubsection[#1]#2{%
  \ifnum \c@secnumdepth >\m@ne
    \refstepcounter{subsubsection}
    \addcontentsline{toc}{subsubsection}{}{#1}
  \else
    \addcontentsline{toc}{subsubsection}{}{#1}
  \fi
  \@makesubsubsectionhead{#2}
  \if@twocolumn\else
    \@afterheading
  \fi} 

\def\@subsubssection#1{%
  \@makesubsubssectionhead{#1}
  \if@twocolumn\else
    \@afterheading
  \fi}

\def\@makesubsubsectionhead#1{%
  \gyo{2\normalbaselineskip}{%
    \parindent\z@
    \leftskip1zw
    \vskip\normalbaselineskip
    \normalsize\gt\bf #1
    \vss}\nobreak}

\let\@makesubsubssectionhead=\@makesubsubsectionhead

