\def\enumerate{%
  \ifnum \@enumdepth >3\relax
    \@toodeep
  \else
    \advance\@enumdepth\@ne
    \ifnum\@enumdepth=\@ne  
      \vskip.5\normalbaselineskip
    \fi
    \edef\@enumctr{enum\romannumeral\the\@enumdepth}%
    \list{\csname label\@enumctr\endcsname}{%
      \topsep\z@
      \ifnum\@enumdepth=\@ne    
        \partopsep.5\normalbaselineskip
      \else
        \partopsep\z@
      \fi
      \parskip\z@
      \itemsep\z@
      \parsep\z@
      \listparindent\z@
      \itemindent\listparindent
      \labelwidth1zw
      \labelsep\z@
      \ifnum\@enumdepth=\@ne
        \leftmargin1zw
      \else
        \leftmargin\leftskip
      \fi
      \advance\leftmargin by 1zw
      \usecounter{\@enumctr}
      \def\makelabel##1{\hss\llap{##1}}}
    \fi}

\let\endenumerate =\endlist

\def\itemize{%
